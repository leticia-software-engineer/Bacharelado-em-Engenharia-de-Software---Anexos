//Questão 2 
//Algoritmo que recebe o salário e o aumento percentual desse valor e exibe o novo salário 
programa
{
	funcao inicio(){
		real salario, aumentopercentual , saldofinal
		leia(salario)
		leia(aumentopercentual)
		saldofinal = salario*aumentopercentual + salario
		escreva(saldofinal)
	}
}
